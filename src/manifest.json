{
  "manifest_version": 2,
  "name": "diegofrayo-chrome-extension",
  "description": "A simple extension to accomplish some useful tasks for me",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/icon-128.png",
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "tabs"],
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  }
}
